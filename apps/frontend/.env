# Public URLs
VITE_API_URL=http://localhost:3000
VITE_APP_URL=http://localhost:5173
VITE_ASSETS_URL=http://localhost:3000/assets

# Public OAuth Client IDs
VITE_GITHUB_CLIENT_ID=your_github_client_id
VITE_GOOGLE_CLIENT_ID=your_google_client_id

# Public API Keys (if needed)
VITE_SENTRY_DSN=your_public_sentry_dsn
VITE_ANALYTICS_ID=your_analytics_id

# WebRTC Public Configuration
VITE_ICE_SERVER_CONFIG={"iceServers":[{"urls":"stun:stun.l.google.com:19302"}]}
VITE_PEER_CONFIG={"debug":2,"host":"localhost","port":3000,"path":"/peer"}

# Feature Flags
VITE_ENABLE_VOICE_INTERFACE=true
VITE_ENABLE_EXPERIMENTAL_FEATURES=false

# Build Configuration
VITE_BUILD_MODE=development
VITE_PUBLIC_PATH=/ 